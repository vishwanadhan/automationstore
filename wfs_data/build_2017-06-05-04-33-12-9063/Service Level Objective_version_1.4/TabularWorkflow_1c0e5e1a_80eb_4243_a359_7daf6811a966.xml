                </user-input>
                <user-input conditionalUserInputValues="true" defaultValue="" description="The Default Service Level (keyword: defaultservicelevel) is the service level definition used if a volume cannot be matched to any of the service level class definitions provided in the configuration file.&#xA;&#xA;The format is:&#xA;defaultservicelevel default-service-level-name" displayName="Default Storage Service Class" locked="false" mandatory="false" name="$DefaultServiceLevel" uuid="a979beeb-f02d-467e-8a19-0d863202298c">
