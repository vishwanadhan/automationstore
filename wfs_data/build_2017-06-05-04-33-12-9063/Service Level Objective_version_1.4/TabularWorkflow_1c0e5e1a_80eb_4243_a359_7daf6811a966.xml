                    <user-input-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="queryUserInputType" query="SELECT&#xA;    CAST(playground.storage_service_level.min_iops AS UNSIGNED) AS &quot;Minimun IOPS&quot;&#xA;FROM&#xA;    playground.storage_service_level&#xA;WHERE&#xA;    '${ModifyServiceLevel}'= 'true'&#xA;    AND playground.storage_service_level.storage_service_name ='${ServiceLevel}'"/>
                </user-input>
                <user-input conditionalUserInputValues="true" defaultValue="" description="Enforce is used to determine the max-throughput value for the volume QoS policy.&#xA;&#xA;There are three enforce modes:&#xA;&#xA;• thick (committed)&#xA;• thin (used)&#xA;• effective (used + space saved by efficiency)&#xA;&#xA;If ignored for a service level, the default enforcement is used." displayName="QoS Policy Enforcement" locked="false" mandatory="false" name="$Enforce" uuid="36bf9df9-eed7-4726-ba62-6a202c2b32b3">
