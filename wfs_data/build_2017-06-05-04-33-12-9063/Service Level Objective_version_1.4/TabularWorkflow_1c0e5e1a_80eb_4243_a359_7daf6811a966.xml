                </user-input>
                <user-input conditionalUserInputValues="true" defaultValue="" description="In the case when a volume matches multiple service level class definitions in the configuration file, the “prefer” keyword is used to select the faster or the slower service level class definitions matching the volume.&#xA;&#xA;Example:&#xA;faster" displayName="Prefer" locked="false" mandatory="false" name="$Prefer" uuid="d6a5620c-125f-4696-839e-722a12d8a2eb">
