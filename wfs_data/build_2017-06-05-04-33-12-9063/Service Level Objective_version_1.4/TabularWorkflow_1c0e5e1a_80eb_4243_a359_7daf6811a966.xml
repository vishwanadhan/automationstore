                    <user-input-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="queryUserInputType" query="SELECT&#xA;    playground.qos_default_values.service_level&#xA;FROM&#xA;    playground.qos_default_values&#xA;UNION&#xA;DISTINCT&#xA;SELECT&#xA;    playground.storage_service_level.storage_service_name AS 'playground.qos_default_values.service_level'&#xA;FROM&#xA;    playground.storage_service_level&#xA;UNION&#xA;DISTINCT&#xA;SELECT&#xA;    &quot;none&quot; AS 'playground.qos_default_values.service_level'"/>
                </user-input>
