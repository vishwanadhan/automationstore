                    <user-input-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="queryUserInputType" query="SELECT&#xA;    playground.storage_service_level.enforce&#xA;FROM&#xA;    playground.storage_service_level&#xA;WHERE&#xA;    playground.storage_service_level.storage_service_name = '${ServiceLevel}'"/>
                </user-input>
                <user-input conditionalUserInputValues="true" defaultValue="" description="Regular expression pattern to use for matching against the volume name.&#xA;&#xA;Example : &quot;exchange|database&quot; &#xA;&#xA;In these examples, only a volume with the patterns of “exchange” or “database” could match this service level.&#xA;&#xA;" displayName="Volume Include (REGEX)" locked="false" mandatory="false" name="$volInclude" uuid="79ec6442-9f48-44d2-8fdb-850c305ef39a">
