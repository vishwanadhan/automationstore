                <user-input defaultValue="Update SSL" description="Add SSL (new SSLC)&#xA;Update SSL (modify existing SSLC)&#xA;Remove SSL (delete SSLC) - (Make sure PSLC referring to this SSLC are updated after deleting this SSLC using workflow to manage PSLC)&#xA;Remove Aggregate from SSL (remove a aggregate from set of mapped aggregate to SSLC, an aggregate can be mapped to only one SSLC)" displayName="Action?" locked="true" mandatory="true" name="$Action" uuid="3dac38ab-b510-479c-aa72-025a400d4431">
                    <user-input-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="enumUserInputType" enumValues="Add SSL,Update SSL,Remove SSL,Remove Aggregate from SSL" viewAsRadio="false"/>
                </user-input>
                <user-input defaultValue="" description="Name of the SSLC" displayName="Storage Service Level Class" locked="false" mandatory="true" name="$StorageServiceName" uuid="99ffcd72-cc5e-44c1-afd7-4f171e3ad02d">
                    <user-input-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="queryUserInputType" query="SELECT&#xA;    playground.storage_service_level.storage_service_name AS 'Name',&#xA;    playground.storage_service_level.iops_throttle AS 'IOPS Throttle(IOPS/TB)',&#xA;    CAST(playground.storage_service_level.thin_min * playground.storage_service_level.iops_throttle / 100 AS SIGNED) AS 'Minimim SLA(IOPS/TB)',&#xA;    playground.storage_service_level.space_guarantee AS 'ThickOrThin',&#xA;    playground.storage_service_level.deduplication_enabled AS 'Dedupe Enabled?',&#xA;    playground.storage_service_level.compression_enabled AS 'Compression Enabled?',&#xA;    playground.storage_service_level.comment AS 'Comment'&#xA;FROM&#xA;    playground.storage_service_level&#xA;WHERE&#xA;    '${Action}' != 'Add SSL'"/>
                </user-input>
                <user-input conditionalUserInputValues="Add SSL,Update SSL" defaultValue="" description="QoS Throttle in Protocol IOPS/TB Stored. This setting will be used by Adaptive QoS Policy manager to manage IOPS usage of volume as per service definition." displayName="SLO Burst Performance (IOPS/TB)" locked="false" mandatory="true" name="$IOPSThrottle" uuid="9aff07e5-52c1-4733-8852-8f8ee337a895">
                    <conditional-user-input>3dac38ab-b510-479c-aa72-025a400d4431</conditional-user-input>
                    <user-input-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="queryUserInputType" query="SELECT&#xA;    playground.storage_service_level.iops_throttle&#xA;FROM&#xA;    playground.storage_service_level&#xA;WHERE&#xA;    '${Action}' != 'Add'&#xA;    AND playground.storage_service_level.storage_service_name = '${StorageServiceName}'"/>
                </user-input>
                <user-input conditionalUserInputValues="Add SSL,Update SSL" defaultValue="" description="Protocol IOPS/TB allocated." displayName="Minimum &quot;SLA&quot; to Consumer (IOPS/TB)" locked="false" mandatory="true" name="$minSLA" uuid="1aed8a1e-af22-4944-9506-8110756ccac8">
                    <conditional-user-input>3dac38ab-b510-479c-aa72-025a400d4431</conditional-user-input>
                    <user-input-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="queryUserInputType" query="SELECT&#xA;    IF(playground.storage_service_level.thin_min IS NOT NULL,&#xA;    CAST(playground.storage_service_level.thin_min * playground.storage_service_level.iops_throttle / 100 AS SIGNED),&#xA;    CAST(playground.qos_default_values.thin_min * playground.storage_service_level.iops_throttle / 100 AS SIGNED)) AS &quot;SLA&quot;&#xA;FROM&#xA;    playground.storage_service_level,&#xA;    playground.qos_default_values&#xA;WHERE&#xA;    '${Action}' != 'Add'&#xA;    AND playground.storage_service_level.storage_service_name = '${StorageServiceName}'"/>
                </user-input>
                <user-input conditionalUserInputValues="Add SSL,Update SSL" defaultValue="" description="Whether volume should be thick provisioned or thin provisioned.&#xA;Thick (No write can fail)&#xA;Thin-Type 1 (Overwrites cannot fail)&#xA;Thin-Type 2 (Both overwrites and hole fills can fail) " displayName="Thick/Thin?" locked="true" mandatory="true" name="$ThickOrThin" uuid="7457b23c-6768-4af3-8970-0da31e94101d">
                    <conditional-user-input>3dac38ab-b510-479c-aa72-025a400d4431</conditional-user-input>
                    <user-input-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="queryUserInputType" query="SELECT&#xA;    playground.storage_service_level.space_guarantee&#xA;FROM&#xA;    playground.storage_service_level&#xA;WHERE&#xA;    '${Action}' != 'Add'&#xA;    AND playground.storage_service_level.storage_service_name = '${StorageServiceName}'&#xA;UNION&#xA;DISTINCT&#xA;SELECT&#xA;    &quot;Thick&quot;&#xA;UNION&#xA;DISTINCT&#xA;SELECT&#xA;    &quot;Thin-Type 1&quot;&#xA;UNION&#xA;DISTINCT&#xA;SELECT&#xA;    &quot;Thin-Type 2&quot;"/>
                </user-input>
                <user-input conditionalUserInputValues="Add SSL,Update SSL" defaultValue="" description="Enables/disables deduplication." displayName="Enable Dedupe?" locked="true" mandatory="true" name="$DedupeEnabled" uuid="20fcd54d-e66a-4965-be5d-296eea2cb262">
                    <conditional-user-input>3dac38ab-b510-479c-aa72-025a400d4431</conditional-user-input>
                    <user-input-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="queryUserInputType" query="SELECT&#xA;    playground.storage_service_level.deduplication_enabled&#xA;FROM&#xA;    playground.storage_service_level&#xA;WHERE&#xA;    '${Action}' != 'Add'&#xA;    AND playground.storage_service_level.storage_service_name = '${StorageServiceName}'&#xA;UNION&#xA;DISTINCT&#xA;SELECT&#xA;    &quot;true&quot;&#xA;UNION&#xA;DISTINCT&#xA;SELECT&#xA;    &quot;false&quot;"/>
                </user-input>
                <user-input conditionalUserInputValues="Add SSL,Update SSL" defaultValue="" description="Configures compression setting on the volume." displayName="Enable Compresion?" locked="true" mandatory="true" name="$CompressionType" uuid="5cbda09a-d869-4dd9-8be6-14cab64bf40b">
                    <conditional-user-input>3dac38ab-b510-479c-aa72-025a400d4431</conditional-user-input>
                    <user-input-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="queryUserInputType" query="SELECT&#xA;    playground.storage_service_level.compression_enabled&#xA;FROM&#xA;    playground.storage_service_level&#xA;WHERE&#xA;    '${Action}' != 'Add'&#xA;    AND playground.storage_service_level.storage_service_name = '${StorageServiceName}'&#xA;UNION&#xA;DISTINCT&#xA;SELECT&#xA;    &quot;inline&quot;&#xA;UNION&#xA;DISTINCT&#xA;SELECT&#xA;    &quot;background&quot;&#xA;UNION&#xA;DISTINCT&#xA;SELECT&#xA;    &quot;disabled&quot;"/>
                </user-input>
                <user-input conditionalUserInputValues="Add SSL,Update SSL" description="Specific comment for a storage service" displayName="Comment" locked="false" mandatory="false" name="$comment" uuid="7401f564-71d8-439e-a57e-294785973a69">
                    <conditional-user-input>3dac38ab-b510-479c-aa72-025a400d4431</conditional-user-input>
                    <user-input-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="stringUserInputType" regularExpression=""/>
                </user-input>
            </user-inputs>
        </user-input-group>
        <user-input-group expandByDefault="true" name="Aggregate Details">
            <user-inputs>
                <user-input conditionalUserInputValues="Add SSL,Update SSL,Add Aggregate to SSL" defaultValue="" displayName="Enable to select aggregate by regular expression" locked="false" mandatory="false" name="$aggrRegExEnabled" uuid="e51e0fdd-6797-4147-b598-d523ebb0b689">
                    <conditional-user-input>3dac38ab-b510-479c-aa72-025a400d4431</conditional-user-input>
                    <user-input-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="queryUserInputType" query="SELECT&#xA;    playground.storage_service_level.aggr_regex_enabled AS 'Regex Eabled'&#xA;FROM&#xA;    playground.storage_service_level&#xA;WHERE&#xA;    playground.storage_service_level.storage_service_name = '${StorageServiceName}'&#xA;UNION&#xA;SELECT&#xA;    'true' AS 'Regex Eabled'&#xA;UNION&#xA;SELECT&#xA;    'false' AS 'Regex Eabled' "/>
                </user-input>
                <user-input conditionalUserInputValues="true" defaultValue="" displayName="Aggregate Include by Regular Expression" locked="false" mandatory="false" name="$aggrIncludeRegex" uuid="17b0876a-bbae-4693-89d5-30829fc074e1">
                    <conditional-user-input>e51e0fdd-6797-4147-b598-d523ebb0b689</conditional-user-input>
                    <user-input-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="queryUserInputType" query="SELECT&#xA;    playground.storage_service_level.aggr_include_regex&#xA;FROM&#xA;    playground.storage_service_level&#xA;WHERE&#xA;    playground.storage_service_level.storage_service_name = '${StorageServiceName}'&#xA;    AND '${aggrRegExEnabled}' = 'true'"/>
                </user-input>
                <user-input conditionalUserInputValues="true" defaultValue="" description="String which is regular expression. Adaptive QoS Policy service will using this to exclude set of aggregates from list of aggregates short listed from other parameters. Volume provisioning is not using this parameter for ignoring aggregates.&#xA;&#xA;E.g.&#xA;&quot;old&quot; value will ignore all the aggregates which got &quot;old&quot; as substring in their aggregate names" displayName="Aggregate Exclude by Regular Expression" locked="false" mandatory="false" name="$aggrExcludeRegex" uuid="bf4a78ca-950a-4fd3-a892-c522c7dcd4b7">
                    <conditional-user-input>e51e0fdd-6797-4147-b598-d523ebb0b689</conditional-user-input>
                    <user-input-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="queryUserInputType" query="SELECT&#xA;    playground.storage_service_level.aggr_exclude_regex&#xA;FROM&#xA;    playground.storage_service_level&#xA;WHERE&#xA;    playground.storage_service_level.storage_service_name = '${StorageServiceName}'&#xA;    AND '${aggrRegExEnabled}' = 'true'"/>
                </user-input>
                <user-input conditionalUserInputValues="false" defaultValue="" description="List of aggregates available and not yet mapped to any of the SSLC.&#xA;Valid only when Aggregate selection by regex is not enabled." displayName="Available Aggregates for mapping" locked="true" mandatory="false" name="$aggrlist" uuid="70ba1151-daf3-4db5-bb33-1e24bea8bf6e">
                    <conditional-user-input>e51e0fdd-6797-4147-b598-d523ebb0b689</conditional-user-input>
                    <user-input-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="queryMultiSelectUserInputType" query="SELECT&#xA;    DISTINCT aggr.name AS 'Aggregate',&#xA;    aggr.state AS 'State',&#xA;    aggr.is_hybrid AS 'Is Hybrid',&#xA;    node.name AS 'Node',&#xA;    cluster.primary_address AS 'Cluster IP',&#xA;    CONCAT(disk.type) AS 'Disk Types',&#xA;    aggr.available_size_mb AS 'Available Capacity(MB)',&#xA;    aggr.size_mb AS 'Total Capacity(MB)',&#xA;    aggr.volume_count AS 'Volume Count'&#xA;FROM&#xA;    cm_storage.aggregate AS aggr&#xA;JOIN&#xA;    cm_storage.node AS node&#xA;        ON aggr.node_id=node.id&#xA;JOIN&#xA;    cm_storage.cluster AS cluster&#xA;        ON node.cluster_id =cluster.id&#xA;JOIN&#xA;    cm_storage.disk_aggregate AS disk_aggrs&#xA;        ON aggr.id =disk_aggrs.aggregate_id&#xA;JOIN&#xA;    cm_storage.disk AS disk&#xA;        ON disk.id = disk_aggrs.disk_id&#xA;WHERE&#xA;    (&#xA;        aggr.has_local_root = 0&#xA;        AND aggr.has_partner_root = 0&#xA;    )&#xA;    AND aggr.name NOT IN (&#xA;        select&#xA;            playground.aggregate_service_map.aggregate_name&#xA;        from&#xA;            playground.aggregate_service_map&#xA;        WHERE&#xA;            cm_storage.cluster.primary_address = playground.aggregate_service_map.cluster_ip&#xA;            AND cm_storage.node.name = playground.aggregate_service_map.node_name&#xA;    )&#xA;    AND '${aggrRegExEnabled}' = 'false'" rowSelectionType="Multiple"/>
                </user-input>
                <user-input conditionalUserInputValues="Remove Aggregate from SSL" defaultValue="" description="List of aggregates which are already mapped to a specific SSLC.&#xA;Valid only when Aggregate selection by regex is not enabled." displayName="Mapped Aggregates" locked="true" mandatory="true" name="$MappedAggrList" uuid="74c7a5a8-3e91-4202-843c-f5182973d2a1">
                    <conditional-user-input>3dac38ab-b510-479c-aa72-025a400d4431</conditional-user-input>
                    <user-input-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="queryMultiSelectUserInputType" query="SELECT&#xA;    aggr.name AS 'Aggregate',&#xA;    node.name AS 'Node',&#xA;    cluster.primary_address AS 'Cluster IP',&#xA;    aggr.available_size_mb AS 'Available Capacity(MB)',&#xA;    aggr.size_mb AS 'Total Capacity(MB)',&#xA;    aggr.number_of_disks AS 'Disk Count',&#xA;    aggr.volume_count AS 'Volume Count',&#xA;    aggr.is_hybrid AS 'Is Hybrid'&#xA;FROM&#xA;    cm_storage.aggregate AS aggr&#xA;JOIN&#xA;    cm_storage.node AS node&#xA;        ON aggr.node_id=node.id&#xA;JOIN&#xA;    cm_storage.cluster AS cluster&#xA;        ON node.cluster_id =cluster.id&#xA;WHERE&#xA;    (&#xA;        aggr.has_local_root = 0&#xA;        AND aggr.has_partner_root = 0&#xA;    )&#xA;    AND (&#xA;        aggr.name IN (&#xA;            select&#xA;                playground.aggregate_service_map.aggregate_name&#xA;            from&#xA;                playground.aggregate_service_map&#xA;            WHERE&#xA;                playground.aggregate_service_map.storage_service_name='${StorageServiceName}'&#xA;                AND cm_storage.cluster.primary_address = playground.aggregate_service_map.cluster_ip&#xA;                AND cm_storage.node.name = playground.aggregate_service_map.node_name&#xA;        )&#xA;    )&#xA;    AND '${aggrRegExEnabled}' = 'false' " rowSelectionType="Multiple"/>
                </user-input>
            </user-inputs>
        </user-input-group>
    </user-input-groups>
    <workflow-constants>
        <workflow-constant name="THIN_PROVISIONING_PERCENT_GAIN" value="30">
            <description>This will be updated in Storage services entry. Change value only when you want to set different value.</description>
        </workflow-constant>
        <workflow-constant name="DEDUPE_CLONE_PERCENT_GAIN" value="50">
            <description>This will be updated in Storage services entry. Change value only when you want to set different value.</description>
        </workflow-constant>
        <workflow-constant name="PRIMARY_POOL" value="'Primary'">
            <description>This is name of the primary pool</description>
        </workflow-constant>
    </workflow-constants>
</tabularWorkflow>
